{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            b.id, \n            b.sender_id, \n            u.username as sender_username,\n            b.content, \n            b.is_anonymous, \n            b.created_at\n        FROM broadcasts b\n        LEFT JOIN users u ON b.sender_id = u.id\n        ORDER BY b.created_at DESC\n        LIMIT $1\n        ",
